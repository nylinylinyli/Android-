<?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".WebActivityBDT20051">
        <EditText
            android:id="@+id/web_textlinuoyan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="默认进入新浪微博官网"
            android:ems="10"
            android:inputType="textPersonName"
            android:text=""
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/btn_yeslinuoyan"
            app:layout_constraintTop_toTopOf="parent"/>

        <Button
            android:id="@+id/btn_yeslinuoyan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="确定"
            app:layout_constraintLeft_toRightOf="@+id/web_textlinuoyan"
            app:layout_constraintTop_toTopOf="parent" />

        <WebView
            android:id="@+id/webviewlinuoyan"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_yeslinuoyan"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <ImageButton
            android:id="@+id/btn_backlinuoyan"
            android:layout_width="80dp"
            android:layout_height="35dp"
            android:layout_marginLeft="16dp"
            android:layout_marginBottom="8dp"
            android:background="@color/purple_500"
            app:layout_constraintBottom_toTopOf="@id/webviewlinuoyan"
            app:layout_constraintLeft_toRightOf="@id/btn_yeslinuoyan"
            app:srcCompat="@android:drawable/ic_menu_revert" />

    </androidx.constraintlayout.widget.ConstraintLayout>